{
  "extends": ["airbnb", "airbnb-typescript", "plugin:prettier/recommended"],
  "parserOptions": {
    "tsconfigRootDir": "./",
    "project": "./tsconfig.json"
  },

  "plugins": ["prettier"],
  "rules": {
    "import/prefer-default-export": 0,
    "import/extensions": [
      "error",
      "never"
    ],
    "prettier/prettier": "error",
    "react/react-in-jsx-scope": 0
  }
}
